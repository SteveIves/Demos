import System
import System.ServiceProcess

namespace MyServiceHostNS
	
	;;; <summary>
	;;; The main entry point for the application.
	;;; </summary>
	{STAThread}
	main 
		record
			servicesToRun, [#]@ServiceBase
		endrecord
	proc
		servicesToRun = new ServiceBase[1]
		servicesToRun[1] = new MyServiceHost()
		ServiceBase.Run(servicesToRun)
	end
	
endnamespace



