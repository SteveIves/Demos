;*******************************************************************************
;
.function D10toPhone ;,a

    a_phone ,n

stack record
    aphone  ,a14

.proc

    clear aphone

    if (a_phone<0)
        a_phone = -a_phone

    if (a_phone) then
        aphone = a_phone ,"(XXX) XXX-XXXX"
    else
        aphone = %char(0)

    freturn aphone

.end

