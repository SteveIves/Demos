Public Sub ClearLic()
Dim iCount As Integer
    For iCount = 0 To 8
        Picture3(iCount).Visible = False
        LblLic(iCount).Visible = False
    Next iCount
    LblServer.Visible = False
    LblDB.Visible = False
End Sub

Public Property Let ServerName(ByVal vNewValue As Variant)
    LblServer.Visible = True
    LblServer.Caption = vNewValue
End Property

Public Property Let SqlName(ByVal vNewValue As Variant)
    LblDB.Visible = True
    LblDB.Caption = vNewValue
End Property

