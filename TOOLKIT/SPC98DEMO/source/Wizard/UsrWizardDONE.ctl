    MMControl1.Command = "Open"
    MMControl1.Command = "Play"
End Sub

Private Sub UserControl_Initialize()
    Call ClearLic
End Sub

Public Property Let NumPSDE(ByVal vNewValue As Long)
    If vNewValue = 0 Then
        LblLic(1).Visible = False
        Picture3(1).Visible = False
    Else
        LblLic(1).Caption = Str(vNewValue) + " PSDE"
        LblLic(1).Visible = True
        Picture3(1).Visible = True
    End If
End Property
