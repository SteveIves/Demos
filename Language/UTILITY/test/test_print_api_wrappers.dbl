        pix_y += 2 * %wpr_info(rpt_hdl, DWP_TEXTHEIGHT)
        pix_x = 100
        result = %wpr_print(rpt_hdl, DWP_WRITEOUT, pix_x, pix_y,
        &                   "Address")
        pix_x = M$PRT_SPEC(pixelwidth)/3
        call draw_field_box
        pix_y += 2 * %wpr_info(rpt_hdl, DWP_TEXTHEIGHT)
        call draw_field_box
        pix_y += 2 * %wpr_info(rpt_hdl, DWP_TEXTHEIGHT)
        call draw_field_box

        pix_y += 2 * %wpr_info(rpt_hdl, DWP_TEXTHEIGHT)
        pix_x = 100
        result = %wpr_print(rpt_hdl, DWP_WRITEOUT, pix_x, pix_y,
        &                   "Telephone")
        pix_x = M$PRT_SPEC(pixelwidth)/3
